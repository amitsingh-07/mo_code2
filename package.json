{
  "name": "bfa-frontend",
  "version": "0.0.0",
  "scripts": {
    "config": "ts-node ./set-env.ts",
    "ng": "ng",
    "start": "npm run config && ng serve -c=common",
    "build": "npm run config && ng build -c=common",
    "test": "npm run config && ng test",
    "lint": "npm run config && ng lint",
    "e2e": "npm run config && ng e2e",
    "postinstall": "ngcc",
    "set:ios:cert": "mkdir -p ios/App/App/www/certificates && cp src/certificates/*.cer ios/App/App/www/certificates && ruby addCertToXcodeProj.rb",
    "set:android:cert": "mkdir -p android/capacitor-cordova-android-plugins/src/main/assets/www/ && cp -r src/certificates android/capacitor-cordova-android-plugins/src/main/assets/www/certificates"
  },
